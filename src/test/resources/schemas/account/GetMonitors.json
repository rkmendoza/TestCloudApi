  ],
  "properties": {
    "monitors": {
      "$id": "#/properties/monitors",
      "type": "array",
      "items": {
        "$id": "#/properties/monitors/items",
        "type": "object",
        "properties": {
          "tags": {
            "$id": "#/properties/monitors/items/properties/tags",
            "type": "array",
            "items": {
              "$id": "#/properties/monitors/items/properties/tags/items",
              "type": "string",
              "pattern": "^(.*)$"
            }
          },
          "focus": {
            "$id": "#/properties/monitors/items/properties/focus",
            "type": "string",
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}
